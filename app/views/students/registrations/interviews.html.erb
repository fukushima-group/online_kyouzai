<%= link_to :back do %>
  <button>戻る</button>
<% end %>
<br>
<% if @student.interviews.present? %>
  <% @student.interviews.each_with_index do |interview, i| %>
    <div class="list">
      <%= "第#{i + 1}回" %>
      <br>
      <%= interview.content%>
    </div>
  <% end %>
<% else %>
  <h3>まだ面談してません</h3>
<% end %>